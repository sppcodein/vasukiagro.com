{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#2E7D32",
						"#795548"
					],
					"name": "Green and Brown",
					"slug": "green-and-brown"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(135deg, #2E7D32 0%, #8BC34A 100%)",
					"name": "Green to Light Green",
					"slug": "green-to-light-green"
				},
				{
					"gradient": "linear-gradient(135deg, #795548 0%, #F9F4E8 100%)",
					"name": "Earth to Sand",
					"slug": "earth-to-sand"
				},
				{
					"gradient": "linear-gradient(135deg, #03A9F4 0%, #FFFFFF 100%)",
					"name": "Sky to White",
					"slug": "sky-to-white"
				}
			],
			"palette": [
				{
					"color": "#2E7D32",
					"name": "Primary Green",
					"slug": "primary-green"
				},
				{
					"color": "#8BC34A",
					"name": "Light Green",
					"slug": "light-green"
				},
				{
					"color": "#795548",
					"name": "Earth Brown",
					"slug": "earth-brown"
				},
				{
					"color": "#F9F4E8",
					"name": "Soft Sand",
					"slug": "soft-sand"
				},
				{
					"color": "#03A9F4",
					"name": "Sky Blue",
					"slug": "sky-blue"
				},
				{
					"color": "#FFC107",
					"name": "Sun Yellow",
					"slug": "sun-yellow"
				},
				{
					"color": "#333333",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#F5F5F5",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1340px"
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				},
				{
					"fontFamily": "'Lora', serif",
					"name": "Lora",
					"slug": "lora"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 900",
							"src": [
								"http://127.0.0.2/wp-content/plugins/woocommerce/assets/fonts/Inter-VariableFont_slnt,wght.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Cardo",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"http://127.0.0.2/wp-content/plugins/woocommerce/assets/fonts/cardo_normal_400.woff2"
							]
						}
					],
					"fontFamily": "Cardo",
					"name": "Cardo",
					"slug": "cardo"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "32px",
					"slug": "x-large"
				},
				{
					"name": "Huge",
					"size": "42px",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary-green)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--primary-green)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "1.5em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"lineHeight": "1.75"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.3"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--primary-green)",
					"style": "solid",
					"width": "0 0 0 4px"
				},
				"spacing": {
					"padding": {
						"left": "1.5rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lora)",
					"fontStyle": "italic"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700",
					"lineHeight": "1.2",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"css": ".wp-block-navigation__responsive-container-content {\n gap: 0!important;\n}\n\n.contact-section,\n.site-footer{\n  margin-block-start: 0;\n}\n\n.main-menu li{\n   padding: 2px!important;\n}\n\n@media screen and (max-width: 600px) {\n    #modal-1-content ul li{\n     margin: 20px!important;\n    }\n}",
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--primary-green)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--primary-green)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--light-green)"
					},
					"outline": {
						"color": "var(--wp--preset--color--primary-green)",
						"style": "dotted",
						"width": "1px"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--light-green)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary-green)"
				}
			}
		},
		"spacing": {
			"blockGap": "2rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "600",
			"lineHeight": "1.6"
		}
	},
	"version": 3
}